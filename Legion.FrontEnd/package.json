{
  "name": "73rdlegionwebsite",
  "version": "0.0.0",
  "description": "Webapp for the 73rd Legion gaming community.  Intended to supplement the interactions within our Discord guild and provide services which Discord does not.  Actual features will be defined and built as we come up with more ideas.",
  "author": "",
  "license": "ISC",
  "keywords": [
    "react",
    "webapp",
    "gaming"
  ],
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack-dev-server --color --progress --config webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blakestone95/73rdLegionWebsite.git"
  },
  "bugs": {
    "url": "https://github.com/blakestone95/73rdLegionWebsite/issues"
  },
  "homepage": "https://github.com/blakestone95/73rdLegionWebsite#readme",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "not dead"
            ]
          }
        }
